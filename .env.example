# GDR Framework V3.1 Enterprise - Configuration
# Copie este arquivo para .env e configure suas APIs

# ============================================
# GOOGLE APIs
# ============================================
# Google Maps/Places API (https://console.cloud.google.com/)
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Google Custom Search API (https://programmablesearchengine.google.com/)
GOOGLE_CSE_API_KEY=your_google_cse_api_key_here
GOOGLE_CSE_ID=your_google_cse_id_here

# ============================================
# LLM PROVIDER APIs
# ============================================
# OpenAI (https://platform.openai.com/api-keys)
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic Claude (https://console.anthropic.com/)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Google Gemini (https://makersuite.google.com/app/apikey)
GEMINI_API_KEY=your_gemini_api_key_here

# DeepSeek (https://platform.deepseek.com/)
DEEPSEEK_API_KEY=your_deepseek_api_key_here

# ZhipuAI (https://open.bigmodel.cn/)
ZHIPUAI_API_KEY=your_zhipuai_api_key_here

# ============================================
# SCRAPING APIs
# ============================================
# Apify (https://console.apify.com/account/integrations)
APIFY_API_KEY=your_apify_api_key_here

# Apify Linktree Scraper (opcional)
APIFY_API_KEY_LINKTREE=your_apify_linktree_key_here

# ============================================
# FACEBOOK (OPCIONAL)
# ============================================
# Facebook Graph API (https://developers.facebook.com/)
FACEBOOK_APP_ID=your_facebook_app_id_here
FACEBOOK_APP_SECRET=your_facebook_app_secret_here
FACEBOOK_ACCESS_TOKEN=your_facebook_access_token_here

# ============================================
# CONFIGURAÇÕES DO SISTEMA
# ============================================
# Modo de execução
DEBUG_MODE=false
LOG_LEVEL=INFO

# Cache DuckDB
USE_CACHE=true
CACHE_TTL_HOURS=168

# Limites de processamento
MAX_CONCURRENT_SCRAPERS=5
MAX_RETRIES=3
REQUEST_TIMEOUT=30

# Controle de custos
MAX_COST_PER_LEAD=0.05
ALERT_COST_THRESHOLD=10.00